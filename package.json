{
	"name": "papyrus-app",
	"version": "0.0.1",
	"private": true,
	"main": "index.cjs",
	"scripts": {
		"vite:dev": "vite dev",
		"vite:build": "vite build",
		"vite:preview": "vite preview",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"ts:build": "rm -rf out; rm index.cjs; swc src-electron -d out; rollup --config rollup.config.mjs",
		"ts:watch": "chokidar \"src-electron/**/*.ts\" -c \"pnpm run ts:build\"",
		"electron:start": "electron .",
		"check": "svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.0.2",
		"@swc/cli": "^0.1.59",
		"@swc/core": "^1.3.27",
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"chokidar-cli": "^3.0.0",
		"electron": "^22.0.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"rollup": "^3.10.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-svelte-svg": "^2.2.1"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.2.3",
		"dayjs": "^1.11.7",
		"form-data": "^4.0.0",
		"nanoid": "^4.0.0",
		"open": "^8.4.0",
		"ws": "^8.12.0"
	}
}
