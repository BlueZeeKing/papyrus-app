{
	"$schema": "https://json.schemastore.org/swcrc",
	"env": {
		"targets": {
			"electron": "22"
		}
	},
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"dynamicImport": false,
			"decorators": false
		},
		"transform": null,
		"target": "es2022",
		"loose": false,
		"externalHelpers": false,
		// Requires v1.2.50 or upper and requires target to be es2016 or upper.
		"keepClassNames": false
	},
	"exclude": [
		"src-electron/papyrus/rollup.config.mjs",
		"src-electron/preload.cjs",
		"src-electron/papyrus/node_modules"
	],
	"minify": false
}
